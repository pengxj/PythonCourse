    "## break和continue的使用"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "for n in range(100, 1, -1):\n",
    "    if n%2==0:\n",
    "        continue\n",
    "    print(n)\n",
    "    for i in range(2, int(n**0.5)):\n",
    "        if n%i == 0:\n",
    "            break\n",
    "    \n",
    "    if i==int(n**0.5)-1:\n",
    "        print(n)\n",
    "        break\n",
    "#     else:\n",
    "#         print(n)\n",
    "#         break"
   ]
  }
